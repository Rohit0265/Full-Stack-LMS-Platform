{
    "version": 2,
    "builds": [{
        "src": "server/Server.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
                "server/config/**",
                "server/model/**",
                "server/controllers/**"
            ]
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "server/Server.js"
    }]
}