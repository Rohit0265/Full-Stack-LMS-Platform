{
    "version": 2,
    "builds": [{
        "src": "Server.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
                "config/**",
                "controllers/**",
                "model/**"
            ]
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "Server.js"
    }]
}